{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "jsonBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%INVOICE_JSON_CONTAINER%/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BLOB_OUTPUT_CONTAINER%/{name}.processed.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}