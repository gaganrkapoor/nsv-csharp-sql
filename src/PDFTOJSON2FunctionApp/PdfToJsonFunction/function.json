{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "blob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "%BLOB_INPUT_CONTAINER%/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "%BLOB_OUTPUT_CONTAINER%/{name}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
